include <wiringPi.h>
#include <softPwm.h>

#define SERVO 29

int main(){
        if(wiringPiSetup() == -1)
                return 1;
        softPwmCreate(SERVO, 0 , 100);

        for(;;){
                softPwmWrite(SERVO, 10);
                delay(1000);
                softPwmWrite(SERVO, 20);
                delay(1000);
        }
}


